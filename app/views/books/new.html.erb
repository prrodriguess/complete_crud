<%= form_with(model: @book) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :author %>
    <%= form.text_field :author %>
  </div>
  <div>
    <%= form.label :publication_year %>
    <%= form.number_field :publication_year %>
  </div>
  <div>
    <%= form.label :genre %>
    <%= form.text_field :genre %>
  </div>
  <%= form.submit %>
<% end %>


